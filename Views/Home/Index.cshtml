@model IEnumerable<LearnASPNETCore.Models.Restaurant>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>ASPNETCORE</title>
</head>
<body>
    <h1>Welcome!</h1>
    <div>This is the home page!</div>

    <table>
        @foreach (var restaurant in Model)
        {
            <tr>
                <td>@restaurant.Id</td>
                <td>@restaurant.Name</td>
            </tr>
        }
    </table>
</body>
</html>